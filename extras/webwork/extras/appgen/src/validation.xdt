<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
    "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
    <XDtMethod:forAllMethods>
    <XDtMethod:ifMethodTagValueEquals tagName="hibernate.property" paramName="not-null" value="true">
    <field name="<XDtForm:classNameLower/>.<XDtMethod:propertyName/>">
        <XDtType:ifIsOfType value="return-type" type="java.util.Date">
        <field-validator type="required">
            <message key="errors.required"/>
        </field-validator>
        <field-validator type="date">
            <message key="errors.date"/>
        </field-validator>
        </XDtType:ifIsOfType>
        <XDtType:ifIsNotOfType value="return-type" type="java.util.Date">
        <field-validator type="requiredstring">
            <message key="errors.required"/>
        </field-validator>    
        </XDtType:ifIsNotOfType>
    </field>
    </XDtMethod:ifMethodTagValueEquals>
    </XDtMethod:forAllMethods>
</validators>
